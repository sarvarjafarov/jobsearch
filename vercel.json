{
    "buildCommand": "bash scripts/vercel-build.sh",
    "outputDirectory": "public",
    "functions": {
        "api/index.php": {
            "runtime": "php@8.2"
        }
    },
    "routes": [
        { "src": "/build/(.*)", "dest": "/build/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ]
}
